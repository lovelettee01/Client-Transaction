CREATE TABLE CLIENT ( 
	ACCT_NO VARCHAR(8) NOT NULL,
	BR_CODE VARCHAR(1) NOT NULL,
	NAME VARCHAR(50) NOT NULL,
	PRIMARY KEY (ACCT_NO)
);

CREATE TABLE TRANSACTION ( 
	SEQ BIGINT AUTO_INCREMENT,
	ACCT_NO VARCHAR(8) NOT NULL,
	AMT VARCHAR(15) NOT NULL,
	CANCEL_YN VARCHAR(1),
	DATE VARCHAR(8) NOT NULL,
	FEE VARCHAR(10),
	IDX VARCHAR(3) NOT NULL
);

CREATE TABLE BRANCH (
	BR_CODE VARCHAR(8) NOT NULL,
	BR_NAME VARCHAR(50) NOT NULL,
	PRIMARY KEY (BR_CODE)
);